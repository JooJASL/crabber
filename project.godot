"GameWorlds": "",
"Player": ""
}

[application]

config/name="Crabber"
config/description="My first android game made in the open-source Godot Game Engine. 2D and simple."
run/main_scene="res://scenes/levels/Main.tscn"
config/icon="res://icon.png"

[display]

