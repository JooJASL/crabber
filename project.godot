config_version=4

_global_script_classes=[ {
"base": "RigidBody2D",
"class": "FallingItem",
"language": "GDScript",
"path": "res://scenes/FallingItem.gd"
}, {
"base": "Node2D",
"class": "GameWorlds",
"language": "GDScript",
"path": "res://scenes/levels/World.gd"
}, {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://scenes/Player.gd"
} ]
